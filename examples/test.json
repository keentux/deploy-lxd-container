{
    "prefix": "ctn-test-",
    "containers": [
        {
            "name": "debian-bullseye",
            "distro":"debian",
            "release":"bullseye",
            "arch":"amd64",
            "storage": "default",
            "mounts": [
                {
                    "uid": "tmp",
                    "from": "/tmp",
                    "to": "/home/devel/tmp"
                }
            ],
            "proxy": [
                {
                    "uid": "haproxy80",
                    "listen_port": "80",
                    "container_port": "80"
                }, {
                    "uid": "haproxy443",
                    "listen_port": "443",
                    "container_port": "443"
                }
            ],
            "packages": [
                "openssh-server"
            ],
            "commands": [
                "03_start_ssh",
                "05_create_user"
            ]
        },
        {
            "name": "centos-stream",
            "distro":"centos",
            "release":"9-Stream",
            "arch":"amd64",
            "storage": "default",
            "mounts": [
                {
                    "uid": "tmp",
                    "from": "/tmp",
                    "to": "/home/devel/tmp"
                }
            ],
            "packages": [
                "openssh-server"
            ],
            "commands": [
                "03_start_ssh",
                "05_create_user"
            ]
        },
        {
            "name": "suse-tumbleweed",
            "distro":"opensuse",
            "release":"tumbleweed",
            "arch":"amd64",
            "storage": "default",
            "mounts": [
                {
                    "uid": "tmp",
                    "from": "/tmp",
                    "to": "/home/devel/tmp"
                }
            ],
            "packages": [
                "openssh"
            ],
            "commands": [
                "03_start_ssh",
                "05_create_user"
            ]
        }
    ]
}